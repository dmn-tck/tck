	<businessKnowledgeModel name="Installment Calculation" id="b_InstallmentCalculation">
		<encapsulatedLogic>
			<formalParameter name="Product Type" typeRef="feel:string"/>
			<formalParameter name="Rate" typeRef="feel:number"/>
			<formalParameter name="Term" typeRef="feel:number"/>
			<formalParameter name="Amount" typeRef="feel:number"/>
			<context>
				<contextEntry>
					<variable name="Monthly Fee"/>
					<literalExpression>
						<text>if &apos;Product Type&apos;="STANDARD LOAN" then 20.00 else if &apos;Product Type&apos;="SPECIAL LOAN" then 25.00 else null</text>
					</literalExpression>
				</contextEntry>
				<contextEntry>
					<variable name="Monthly Repayment"/>
					<literalExpression>
						<text>(Amount *Rate/12) / (1 - (1 + Rate/12)**-Term)</text>
					</literalExpression>
				</contextEntry>
				<contextEntry>
					<literalExpression>
						<text>&apos;Monthly Repayment&apos; + &apos;Monthly Fee&apos;</text>
					</literalExpression>
				</contextEntry>
			</context>
		</encapsulatedLogic>
		<variable name="Installment Calculation"/>
	</businessKnowledgeModel>
	<businessKnowledgeModel name="Routing Rules" id="b_RoutingRules">
		<encapsulatedLogic>
			<formalParameter name="Bankrupt" typeRef="feel:boolean"/>
			<formalParameter name="Credit Score" typeRef="feel:number"/>
			<formalParameter name="Post-bureau Risk Category" typeRef="tns:tRiskCategory"/>
			<formalParameter name="Post-bureau Affordability" typeRef="feel:boolean"/>
			<decisionTable hitPolicy="PRIORITY">
				<input>
					<inputExpression>
						<text>&apos;Post-bureau Risk Category&apos;</text>
					</inputExpression>
				</input>
				<input>
					<inputExpression>
						<text>&apos;Post-bureau Affordability&apos;</text>
					</inputExpression>
				</input>
				<input>
					<inputExpression>
						<text>Bankrupt</text>
					</inputExpression>
				</input>
				<input>
					<inputExpression>
						<text>&apos;Credit Score&apos;</text>
					</inputExpression>
				</input>
				<output>
					<outputValues>
						<text>&quot;DECLINE&quot;,&quot;REFER&quot;, &quot;ACCEPT&quot;</text>
					</outputValues>
				</output>
				<rule>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<inputEntry>
						<text>false</text>
					</inputEntry>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<outputEntry>
						<text>"DECLINE"</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<inputEntry>
						<text>true</text>
					</inputEntry>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<outputEntry>
						<text>"DECLINE"</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>
						<text>"HIGH"</text>
					</inputEntry>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<outputEntry>
						<text>"REFER"</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<inputEntry>
						<text>&lt;580</text>
					</inputEntry>
					<outputEntry>
						<text>"REFER"</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<inputEntry>
						<text>-</text>
					</inputEntry>
					<outputEntry>
						<text>"ACCEPT"</text>
					</outputEntry>
				</rule>
			</decisionTable>
		</encapsulatedLogic>
		<variable name="Routing Rules"/>
	</businessKnowledgeModel>
