		<informationRequirement>
			<requiredInput href="#i_ApplicantData"/>
		</informationRequirement>
		<knowledgeRequirement>
			<requiredKnowledge href="#b_ApplicationRiskScoreModel"/>
		</knowledgeRequirement>
