<?xml version="1.0" encoding="UTF-8"?>
<testCases xmlns="http://www.omg.org/spec/DMN/20160719/testcase" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelName>0118-multi-sum-collect-hitpolicy.dmn</modelName>
	<labels>
		<label>Compliance Level 2</label>
		<label>Item Definition</label>
		<label>Hit Policy: COLLECT</label>
		<label>Aggregator: SUM</label>
		<label>Decision Table: Multiple Output Columns</label>
		<label>Data Type: Number</label>
		<label>Data Type: Structure</label>
	</labels>
	<testCase id="001">
		<inputNode name="NumOfYears">
			<value>4</value>
		</inputNode>
		<inputNode name="CustomerType">
			<value>VIP</value>
		</inputNode>
		<resultNode name="CarInsurance" type="decision">
			<expected>
				<component name="CarInsuranceAmount">
					<value>355.64</value>
				</component>
				<component name="AgentFees">
					<value>700</value>
				</component>
			</expected>
		</resultNode>
	</testCase>
