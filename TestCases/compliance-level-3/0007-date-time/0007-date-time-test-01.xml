<?xml version="1.0" encoding="UTF-8"?>
<testCases xmlns="http://www.omg.org/spec/DMN/20160719/testcase" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.omg.org/spec/DMN/20160719/testcase ../testCases.xsd">
	<modelName>0007-date-time.dmn</modelName>
	<testCase id="001">
		<inputNode name="dateString">
			<value>2015-12-24</value>
		</inputNode>
		<inputNode name="timeString">
			<value>00:00:01-01:00</value>
		</inputNode>
		<inputNode name="dateTimeString">
			<value>2016-12-24T23:59:00-08:00</value>
		</inputNode>
		<inputNode name="Hours">
			<value>12</value>
		</inputNode>
		<inputNode name="Minutes">
			<value>59</value>
		</inputNode>
		<inputNode name="Seconds">
			<value>01.3</value>
		</inputNode>
		<inputNode name="Timezone">
			<value>-PT1H</value>
		</inputNode>
		<inputNode name="Year">
			<value>1999</value>
		</inputNode>
		<inputNode name="Month">
			<value>11</value>
		</inputNode>
		<inputNode name="Day">
			<value>22</value>
		</inputNode>
		<inputNode name="oneHour">
			<value>PT1H</value>
		</inputNode>
		<inputNode name="durationString">
			<value>P13DT2H14S</value>
		</inputNode>
		<resultNode name="Date-Time" type="decision">
			<expected>
				<value>2016-12-24T23:59:00-08:00</value>
			</expected>
		</resultNode>
		<resultNode name="Date" type="decision">
			<expected>
				<component name="fromString">
					<value>2015-12-24</value>
				</component>
				<component name="fromDateTime">
					<value>2016-12-24</value>
				</component>
				<component name="fromYearMonthDay">
					<value>1999-11-22</value>
				</component>
			</expected>
		</resultNode>
		<resultNode name="Time" type="decision">
			<expected>
				<value>00:00:01-01:00</value>
			</expected>
		</resultNode>
		<resultNode name="Date-Time2" type="decision">
			<expected>
				<value>2015-12-24T00:00:01-01:00</value>
			</expected>
		</resultNode>
		<resultNode name="Time2" type="decision">
			<expected>
				<value>00:00:01-01:00</value>
			</expected>
		</resultNode>
		<resultNode name="Time3" type="decision">
			<expected>
				<value>12:59:01.3-01:00</value>
			</expected>
		</resultNode>
		<resultNode name="dtDuration1" type="decision">
			<expected>
				<value>P13DT2H14S</value>
			</expected>
		</resultNode>
		<resultNode name="dtDuration2" type="decision">
			<expected>
				<value>P367DT6H58M59S</value>
			</expected>
		</resultNode>
		<resultNode name="hoursInDuration" type="decision">
			<expected>
				<value>8814.98305555556</value>
			</expected>
		</resultNode>
		<resultNode name="sumDurations" type="decision">
			<expected>
				<value>P380DT8H59M13S</value>
			</expected>
		</resultNode>
		<resultNode name="ymDuration2" type="decision">
			<expected>
				<value>P1Y0M</value>
			</expected>
		</resultNode>
		<resultNode name="cDay" type="decision">
			<expected>
				<value>24</value>
			</expected>
		</resultNode>
		<resultNode name="cYear" type="decision">
			<expected>
				<value>2015</value>
			</expected>
		</resultNode>
		<resultNode name="cMonth" type="decision">
			<expected>
				<value>12</value>
			</expected>
		</resultNode>
		<resultNode name="cHour" type="decision">
			<expected>
				<value>0</value>
			</expected>
		</resultNode>
		<resultNode name="cMinute" type="decision">
			<expected>
				<value>0</value>
			</expected>
		</resultNode>
		<resultNode name="cSecond" type="decision">
			<expected>
				<value>1</value>
			</expected>
		</resultNode>
		<resultNode name="cTimezone" type="decision">
			<expected>
				<value>-PT1H</value>
			</expected>
		</resultNode>
		<resultNode name="years" type="decision">
			<expected>
				<value>1</value>
			</expected>
		</resultNode>
		<resultNode name="seconds" type="decision">
			<expected>
				<value>14</value>
			</expected>
		</resultNode>
	</testCase>
</testCases>
